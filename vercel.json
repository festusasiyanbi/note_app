{
	"version": 2,
	"routes": [
	  { "src": "/static/(.*)", "dest": "/website/static/$1" },
	  { "src": "/(.*)", "dest": "/main.py" }
	],
	"rewrites": [
	  { "source": "/(.*)", "destination": "/main.py" }
	],
	"builds": [
	  { "src": "/website/(.*)", "use": "@vercel/static" },
	  { "src": "/instance/(.*)", "use": "@vercel/static" },
	  { "src": "/(.*)", "use": "@vercel/python" }
	]
  }
  